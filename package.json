{ "name": "meteorite"
, "version": "0.4.1"
, "homepage": "http://oortcloud.github.com/meteorite"
, "engines": [ "node >=0.6.0" ]
, "description": "Installer & smart package manager for Meteor"
, "author": "Mike Bannister <notimpossiblemike@gmail.com> (http://po.ssibiliti.es)"
, "dependencies": 
  { "ddp": ">=0.2.1"
  , "underscore": ">=1.3.3"
  , "wrench": ">=1.3.9"
  , "fstream": ">=0.1.18"
  , "optimist": ">=0.3.4"
  , "prompt": "0.2.3"
  , "colors": "0.6.0-1"
  }
, "devDependencies": 
  { "mocha": ">=1.2.2"
  , "connect": ">=2.3.6"
  }
, "scripts":
  { "test": "mocha spec/unit spec/acceptance -t 240000 -R spec"
  , "flushcache": "rm -rf spec/support/cache"
  }
, "repository":
  { "type" : "git"
  , "url" : "https://github.com/oortcloud/meteorite.git"
  }
, "bin" : { "mrt" : "./bin/mrt.js" }
}
